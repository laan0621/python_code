import random
c = 0
str = []
while c < 20:
    str.append(random.randint(1,20))
    c+=1
str.sort()
print(str)
a = []
c = 0
str.sort()
while c < len(str) :
    if c + 1 < len(str):
        a.append([str[c+1]-str[c],str[c],str[c+1]])
    c+=1
c = 0
a.sort()
str1 = []
while c < len(a):
    if a[c][0] == a[0][0] and not [a[c][1],a[c][2]] in str1:
        print('(%d, %d)'%(a[c][1],a[c][2]))
        str1.append([a[c][1],a[c][2]])
    c+=1
